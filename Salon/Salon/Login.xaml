        <Image HorizontalAlignment="Left" Height="281" VerticalAlignment="Top" Width="506" Source="SLIKE/3login3.jpg" Margin="0,0,-16,-12"/>
        <Label x:Name="lbUser" Content="USERNAME :" HorizontalAlignment="Left" Height="28" Margin="67,113,0,0" VerticalAlignment="Top" Width="80" Foreground="#FFE8DDDD"/>
        <Label x:Name="lbPass" Content="PASSWORD :" HorizontalAlignment="Left" Height="28" Margin="67,165,0,0" VerticalAlignment="Top" Width="80" Foreground="#FFECDDDD"/>
        <TextBox x:Name="tbKorIme" HorizontalAlignment="Left" Height="28" Margin="188,113,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="154"/>
        <Button Content="DALJE" HorizontalAlignment="Left" Height="182" Margin="378,46,0,0" VerticalAlignment="Top" Width="70" Background="{x:Null}" Foreground="#FF111010" Click="Button_Click"/>
        <PasswordBox x:Name="pbPass" HorizontalAlignment="Left" Height="28" Margin="188,165,0,0" VerticalAlignment="Top" Width="154"/>

    </Grid>
</Window>
